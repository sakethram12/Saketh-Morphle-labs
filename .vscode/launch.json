{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Flask",
        "type": "python",
        "request": "launch",
        "program": "app.py",
        "console": "integratedTerminal",
        "env": {
          "FLASK_APP": "app.py"
        },
        "args": [
          "run",
          "--no-debugger",
          "--no-reload",
          "--host=0.0.0.0",
          "--port=5000"
        ],
        "jinja": true,
        "justMyCode": true
      }
    ]
  }
  
